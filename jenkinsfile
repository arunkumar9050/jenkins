pipeline {
    agent any

  stages {
     stage('Build') {
       steps {
         echo 'qqweqw'
	 sh 'qwerty'
	}
      }
      stage('Test'){
        steps {
	  echo 'qweqwew'
	  sh 'qwert'
	}
      }
      stage('Deploy') {
        steps {
	  echo 'eqweqweqwe'
	  sh 'qwer'
	 }
       }
     }
   }

